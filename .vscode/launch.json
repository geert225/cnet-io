{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ST-Link: Debug Bootloader",
      "cwd": "${workspaceFolder}/src",
      "executable": "./main.elf",
      "servertype": "stutil",
      "request": "launch",
      "type": "cortex-debug",
      "device": "STM32F439ZI",
      "runToEntryPoint": "main",
      "interface": "swd",
      "preLaunchTask": "build_firmware"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/geert/Documents/GitHub/cnet-io/firmware/src",
      "program": "c:/Users/geert/Documents/GitHub/cnet-io/firmware/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}